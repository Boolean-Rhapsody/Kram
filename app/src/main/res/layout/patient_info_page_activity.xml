<android.support.constraint.ConstraintLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:id="@+id/patient_info_page_constraint_layout"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:background="@color/patient_info_page_activity_patient_info_page_constraint_layout_background_color">

	<View
		android:id="@+id/title_constraint_layout"
		android:layout_width="0dp"
		android:layout_height="@dimen/patient_info_page_activity_title_constraint_layout_height"
		android:background="#FFFFFF"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintHorizontal_weight="1"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintRight_toRightOf="parent"
		tools:layout_editor_absoluteX="0dp"
		tools:layout_editor_absoluteY="560dp"/>

	<View
		android:id="@+id/rectangle_constraint_layout"
		android:layout_width="@dimen/patient_info_page_activity_rectangle_constraint_layout_width"
		android:layout_height="@dimen/patient_info_page_activity_rectangle_constraint_layout_height"
		android:background="#FFFFFF"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintRight_toRightOf="parent"
		tools:layout_editor_absoluteX="180dp"
		tools:layout_editor_absoluteY="560dp"/>

	<View
		android:id="@+id/rectangle_two_constraint_layout"
		android:layout_width="@dimen/patient_info_page_activity_rectangle_two_constraint_layout_width"
		android:layout_height="@dimen/patient_info_page_activity_rectangle_two_constraint_layout_height"
		android:background="#FFFFFF"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintLeft_toLeftOf="parent"
		tools:layout_editor_absoluteX="0dp"
		tools:layout_editor_absoluteY="560dp"/>

	<ImageView
		android:id="@+id/screen_shot20200319_at54904_pm_image_view"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginBottom="@dimen/patient_info_page_activity_screen_shot20200319_at54904_pm_image_view_margin_bottom"
		android:layout_marginEnd="@dimen/patient_info_page_activity_screen_shot20200319_at54904_pm_image_view_margin_end"
		android:scaleType="center"
		android:src="@drawable/screen_shot_2020_03_19_at_54904_pm"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintRight_toRightOf="parent"
		tools:layout_editor_absoluteX="246dp"
		tools:layout_editor_absoluteY="576dp"/>

	<ImageView
		android:id="@+id/screen_shot20200320_at32917_pm_image_view"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginBottom="@dimen/patient_info_page_activity_screen_shot20200320_at32917_pm_image_view_margin_bottom"
		android:layout_marginEnd="@dimen/patient_info_page_activity_screen_shot20200320_at32917_pm_image_view_margin_end"
		android:scaleType="centerCrop"
		android:src="@drawable/screen_shot_2020_03_20_at_32917_pm"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintRight_toRightOf="parent"
		tools:layout_editor_absoluteX="462dp"
		tools:layout_editor_absoluteY="789dp"/>

	<ImageView
		android:id="@+id/screen_shot20200320_at33119_pm_image_view"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginBottom="@dimen/patient_info_page_activity_screen_shot20200320_at33119_pm_image_view_margin_bottom"
		android:layout_marginStart="@dimen/patient_info_page_activity_screen_shot20200320_at33119_pm_image_view_margin_start"
		android:scaleType="center"
		android:src="@drawable/screen_shot_2020_03_20_at_33119_pm"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintLeft_toLeftOf="parent"
		tools:layout_editor_absoluteX="60dp"
		tools:layout_editor_absoluteY="570dp"/>

	<android.support.v7.widget.Toolbar
		android:id="@+id/toolbar"
		android:layout_width="0dp"
		android:layout_height="wrap_content"
		android:background="#C4ADF9"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintRight_toRightOf="parent"
		app:layout_constraintTop_toTopOf="parent"/>
</android.support.constraint.ConstraintLayout>