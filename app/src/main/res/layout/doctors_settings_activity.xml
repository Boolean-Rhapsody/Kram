<layout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools">

	<data/>

	<android.support.constraint.ConstraintLayout
		android:id="@+id/doctorssettings_constraint_layout"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:background="@color/doctors_settings_activity_doctorssettings_constraint_layout_background_color">
	
		<android.support.constraint.ConstraintLayout
			android:id="@+id/information_constraint_layout"
			android:layout_width="0dp"
			android:layout_height="@dimen/doctors_settings_activity_information_constraint_layout_height"
			android:layout_marginTop="@dimen/doctors_settings_activity_information_constraint_layout_margin_top"
			app:layout_constraintHorizontal_weight="1"
			app:layout_constraintLeft_toLeftOf="parent"
			app:layout_constraintRight_toRightOf="parent"
			app:layout_constraintTop_toBottomOf="@+id/avatar_temp_image_view"
			tools:layout_editor_absoluteX="0dp"
			tools:layout_editor_absoluteY="199dp">
		
			<android.support.constraint.ConstraintLayout
				android:id="@+id/ui_settings_cell_constraint_layout"
				android:layout_width="0dp"
				android:layout_height="@dimen/doctors_settings_activity_ui_settings_cell_constraint_layout_height"
				android:layout_marginTop="@dimen/doctors_settings_activity_ui_settings_cell_constraint_layout_margin_top"
				android:background="#FFFFFF"
				app:layout_constraintHorizontal_weight="1"
				app:layout_constraintLeft_toLeftOf="parent"
				app:layout_constraintRight_toRightOf="parent"
				app:layout_constraintTop_toBottomOf="@+id/details_text_view"
				tools:layout_editor_absoluteX="0dp"
				tools:layout_editor_absoluteY="25dp">
			
				<ImageView
					android:id="@+id/path2_image_view"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginEnd="@dimen/doctors_settings_activity_path2_image_view_margin_end"
					android:scaleType="center"
					android:src="@drawable/path_2_3"
					app:layout_constraintBottom_toBottomOf="parent"
					app:layout_constraintRight_toRightOf="parent"
					app:layout_constraintTop_toTopOf="parent"
					tools:layout_editor_absoluteX="348dp"
					tools:layout_editor_absoluteY="23dp"/>
			
				<TextView
					android:id="@+id/patient_name_edit"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginEnd="@dimen/doctors_settings_activity_text_text_view_margin_end"
					android:fontFamily="@font/font_lato_bold"
					android:gravity="end"
					android:text="@string/doctors_settings_activity_text_text_view_text"
					android:textColor="#D9676E"
					android:textSize="@dimen/doctors_settings_activity_text_text_view_text_size"
					app:layout_constraintBottom_toBottomOf="parent"
					app:layout_constraintRight_toLeftOf="@+id/path2_image_view"
					app:layout_constraintTop_toTopOf="parent"
					tools:layout_editor_absoluteX="260dp"
					tools:layout_editor_absoluteY="20dp"/>
			
				<TextView
					android:id="@+id/label_text_view"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginStart="@dimen/doctors_settings_activity_label_text_view_margin_start"
					android:fontFamily="@font/font_lato_regular"
					android:gravity="start"
					android:text="@string/doctors_settings_activity_label_text_view_text"
					android:textColor="@color/doctors_settings_activity_label_text_view_text_color"
					android:textSize="@dimen/doctors_settings_activity_label_text_view_text_size"
					app:layout_constraintBottom_toBottomOf="parent"
					app:layout_constraintLeft_toLeftOf="parent"
					app:layout_constraintTop_toTopOf="parent"
					tools:layout_editor_absoluteX="20dp"
					tools:layout_editor_absoluteY="20dp"/>
			</android.support.constraint.ConstraintLayout>

			<android.support.constraint.ConstraintLayout
				android:visibility="gone"
				android:id="@+id/ui_settings_cell_two_constraint_layout"
				android:layout_width="0dp"
				android:layout_height="@dimen/doctors_settings_activity_ui_settings_cell_two_constraint_layout_height"
				android:background="#FFFFFF"
				app:layout_constraintHorizontal_weight="1"
				app:layout_constraintLeft_toLeftOf="parent"
				app:layout_constraintRight_toRightOf="parent"
				app:layout_constraintTop_toBottomOf="@+id/ui_settings_cell_constraint_layout"
				tools:layout_editor_absoluteX="0dp"
				tools:layout_editor_absoluteY="85dp">
			
				<ImageView
					android:id="@+id/path2_two_image_view"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginEnd="@dimen/doctors_settings_activity_path2_two_image_view_margin_end"
					android:scaleType="center"
					android:src="@drawable/path_2_3"
					app:layout_constraintBottom_toBottomOf="parent"
					app:layout_constraintRight_toRightOf="parent"
					app:layout_constraintTop_toTopOf="parent"
					tools:layout_editor_absoluteX="348dp"
					tools:layout_editor_absoluteY="23dp"/>
			
				<TextView
					android:id="@+id/patient_severity_edit"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginEnd="@dimen/doctors_settings_activity_text_two_text_view_margin_end"
					android:fontFamily="@font/font_lato_bold"
					android:gravity="end"
					android:text="@string/doctors_settings_activity_text_two_text_view_text"
					android:textColor="#D9676E"
					android:textSize="@dimen/doctors_settings_activity_text_two_text_view_text_size"
					app:layout_constraintBottom_toBottomOf="parent"
					app:layout_constraintRight_toLeftOf="@+id/path2_two_image_view"
					app:layout_constraintTop_toTopOf="parent"
					tools:layout_editor_absoluteX="279dp"
					tools:layout_editor_absoluteY="20dp"/>
			
				<TextView
					android:id="@+id/label_two_text_view"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginStart="@dimen/doctors_settings_activity_label_two_text_view_margin_start"
					android:fontFamily="@font/font_lato_regular"
					android:gravity="start"
					android:text="@string/doctors_settings_activity_label_two_text_view_text"
					android:textColor="@color/doctors_settings_activity_label_two_text_view_text_color"
					android:textSize="@dimen/doctors_settings_activity_label_two_text_view_text_size"
					app:layout_constraintBottom_toBottomOf="parent"
					app:layout_constraintLeft_toLeftOf="parent"
					app:layout_constraintTop_toTopOf="parent"
					tools:layout_editor_absoluteX="20dp"
					tools:layout_editor_absoluteY="20dp"/>
			</android.support.constraint.ConstraintLayout>
		
			<TextView
				android:id="@+id/details_text_view"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginStart="@dimen/doctors_settings_activity_details_text_view_margin_start"
				android:alpha="0.4"
				android:fontFamily="@font/font_lato_regular"
				android:gravity="start"
				android:letterSpacing="-0"
				android:text="@string/doctors_settings_activity_details_text_view_text"
				android:textColor="#000000"
				android:textSize="@dimen/doctors_settings_activity_details_text_view_text_size"
				app:layout_constraintLeft_toLeftOf="parent"
				app:layout_constraintTop_toTopOf="parent"
				tools:layout_editor_absoluteX="20dp"
				tools:layout_editor_absoluteY="0dp"/>
		
			<TextView
				android:id="@+id/information_text_view"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginStart="@dimen/doctors_settings_activity_information_text_view_margin_start"
				android:layout_marginTop="@dimen/doctors_settings_activity_information_text_view_margin_top"
				android:alpha="0.4"
				android:fontFamily="@font/font_lato_regular"
				android:gravity="start"
				android:letterSpacing="-0"
				android:text="@string/doctors_settings_activity_information_text_view_text"
				android:textColor="#000000"
				android:textSize="@dimen/doctors_settings_activity_information_text_view_text_size"
				app:layout_constraintLeft_toLeftOf="parent"
				app:layout_constraintTop_toBottomOf="@+id/ui_settings_cell_two_constraint_layout"
				tools:layout_editor_absoluteX="20dp"
				tools:layout_editor_absoluteY="175dp"/>

			<android.support.constraint.ConstraintLayout
				android:id="@+id/group2_constraint_layout"
				android:layout_width="0dp"
				android:layout_height="@dimen/doctors_settings_activity_group2_constraint_layout_height"
				android:background="#FFFFFF"
				app:layout_constraintBottom_toBottomOf="parent"
				app:layout_constraintHorizontal_weight="1"
				app:layout_constraintLeft_toLeftOf="parent"
				app:layout_constraintRight_toRightOf="parent"
				tools:layout_editor_absoluteX="0dp"
				android:visibility="gone"
				tools:layout_editor_absoluteY="260dp">

				<TextView
					android:id="@+id/label_three_text_view"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginStart="@dimen/doctors_settings_activity_label_three_text_view_margin_start"
					android:fontFamily="@font/font_lato_regular"
					android:gravity="start"
					android:text="@string/doctors_settings_activity_label_three_text_view_text"
					android:textColor="@color/doctors_settings_activity_label_three_text_view_text_color"
					android:textSize="@dimen/doctors_settings_activity_label_three_text_view_text_size"
					app:layout_constraintBottom_toBottomOf="parent"
					app:layout_constraintLeft_toLeftOf="parent"
					app:layout_constraintTop_toTopOf="parent"
					tools:layout_editor_absoluteX="20dp"
					tools:layout_editor_absoluteY="20dp"/>

				<Switch
					android:id="@+id/slide_switch"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginEnd="@dimen/doctors_settings_activity_slide_switch_margin_end"
					android:checked="true"
					android:theme="@style/DoctorsSettingsActivitySlideSwitchTheme"
					android:thumbTint="@color/doctors_settings_activity_slide_switch_thumb_tint_color"
					app:layout_constraintBottom_toBottomOf="parent"
					app:layout_constraintRight_toRightOf="parent"
					app:layout_constraintTop_toTopOf="parent"
					tools:layout_editor_absoluteX="304dp"
					tools:layout_editor_absoluteY="14dp"/>
			</android.support.constraint.ConstraintLayout>

			<android.support.constraint.ConstraintLayout
				android:id="@+id/group2_two_constraint_layout"
				android:layout_width="0dp"
				android:layout_height="@dimen/doctors_settings_activity_group2_two_constraint_layout_height"
				android:layout_marginTop="@dimen/doctors_settings_activity_group2_two_constraint_layout_margin_top"
				android:background="#FFFFFF"
				app:layout_constraintHorizontal_weight="1"
				app:layout_constraintLeft_toLeftOf="parent"
				app:layout_constraintRight_toRightOf="parent"
				app:layout_constraintTop_toBottomOf="@+id/information_text_view"
				tools:layout_editor_absoluteX="0dp"
				tools:layout_editor_absoluteY="200dp">
			
				<TextView
					android:id="@+id/label_four_text_view"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginStart="@dimen/doctors_settings_activity_label_four_text_view_margin_start"
					android:fontFamily="@font/font_lato_regular"
					android:gravity="start"
					android:text="@string/doctors_settings_activity_label_four_text_view_text"
					android:textColor="@color/doctors_settings_activity_label_four_text_view_text_color"
					android:textSize="@dimen/doctors_settings_activity_label_four_text_view_text_size"
					app:layout_constraintBottom_toBottomOf="parent"
					app:layout_constraintLeft_toLeftOf="parent"
					app:layout_constraintTop_toTopOf="parent"
					tools:layout_editor_absoluteX="20dp"
					tools:layout_editor_absoluteY="20dp"/>
			
				<Switch
					android:id="@+id/slide_two_switch"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginEnd="@dimen/doctors_settings_activity_slide_two_switch_margin_end"
					android:checked="true"
					android:theme="@style/DoctorsSettingsActivitySlideTwoSwitchTheme"
					android:thumbTint="@color/doctors_settings_activity_slide_two_switch_thumb_tint_color"
					app:layout_constraintBottom_toBottomOf="parent"
					app:layout_constraintRight_toRightOf="parent"
					app:layout_constraintTop_toTopOf="parent"
					tools:layout_editor_absoluteX="304dp"
					tools:layout_editor_absoluteY="14dp"/>
			</android.support.constraint.ConstraintLayout>
		</android.support.constraint.ConstraintLayout>
	
		<ImageView
			android:id="@+id/avatar_temp_image_view"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginTop="@dimen/doctors_settings_activity_avatar_temp_image_view_margin_top"
			android:scaleType="center"
			android:src="@drawable/avatar_temp"
			app:layout_constraintLeft_toLeftOf="parent"
			app:layout_constraintRight_toRightOf="parent"
			app:layout_constraintTop_toTopOf="parent"
			tools:layout_editor_absoluteX="138dp"
			tools:layout_editor_absoluteY="88dp"/>
	
		<android.support.v7.widget.Toolbar
			android:id="@+id/toolbar"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			app:layout_constraintLeft_toLeftOf="parent"
			app:layout_constraintRight_toRightOf="parent"
			app:layout_constraintTop_toTopOf="parent"/>
	</android.support.constraint.ConstraintLayout>
</layout>