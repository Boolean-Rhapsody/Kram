<android.support.constraint.ConstraintLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:id="@+id/patient_sign_up_constraint_layout"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:background="@color/patient_sign_up_activity_patient_sign_up_constraint_layout_background_color">

	<TextView
		android:id="@+id/login_text_view"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginTop="@dimen/patient_sign_up_activity_login_text_view_margin_top"
		android:fontFamily="@font/font_arialmt"
		android:gravity="start"
		android:text="@string/patient_sign_up_activity_login_text_view_text"
		android:textColor="#C4ADF9"
		android:textSize="@dimen/patient_sign_up_activity_login_text_view_text_size"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintRight_toRightOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		tools:layout_editor_absoluteX="131dp"
		tools:layout_editor_absoluteY="106dp"/>

	<TextView
		android:id="@+id/id_code_text_view"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginStart="@dimen/patient_sign_up_activity_id_code_text_view_margin_start"
		android:layout_marginTop="@dimen/patient_sign_up_activity_id_code_text_view_margin_top"
		android:fontFamily="@font/font_arialmt"
		android:gravity="start"
		android:text="@string/patient_sign_up_activity_id_code_text_view_text"
		android:textColor="#C4ADF9"
		android:textSize="@dimen/patient_sign_up_activity_id_code_text_view_text_size"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		tools:layout_editor_absoluteX="25dp"
		tools:layout_editor_absoluteY="200dp"/>

	<EditText
		android:id="@+id/rectangle_edit_text"
		android:layout_width="@dimen/patient_sign_up_activity_rectangle_edit_text_width"
		android:layout_height="@dimen/patient_sign_up_activity_rectangle_edit_text_height"
		android:layout_marginEnd="@dimen/patient_sign_up_activity_rectangle_edit_text_margin_end"
		android:layout_marginTop="@dimen/patient_sign_up_activity_rectangle_edit_text_margin_top"
		android:background="#32C4ADF9"
		android:gravity="start|center_vertical"
		android:inputType="text"
		android:maxLines="1"
		android:textColor="@color/patient_sign_up_activity_rectangle_edit_text_text_color"
		android:textSize="@dimen/patient_sign_up_activity_rectangle_edit_text_text_size"
		app:layout_constraintRight_toRightOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/login_text_view"
		tools:layout_editor_absoluteX="140dp"
		tools:layout_editor_absoluteY="200dp"/>

	<Button
		android:id="@+id/submit_button"
		android:layout_width="@dimen/patient_sign_up_activity_submit_button_width"
		android:layout_height="@dimen/patient_sign_up_activity_submit_button_height"
		android:layout_marginEnd="@dimen/patient_sign_up_activity_submit_button_margin_end"
		android:layout_marginTop="@dimen/patient_sign_up_activity_submit_button_margin_top"
		android:background="@drawable/patient_sign_up_activity_submit_button_ripple"
		android:fontFamily="@font/font_arialmt"
		android:gravity="start"
		android:text="@string/patient_sign_up_activity_submit_button_text"
		android:textColor="#FFFFFF"
		android:textSize="@dimen/patient_sign_up_activity_submit_button_text_size"
		app:layout_constraintRight_toRightOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/rectangle_edit_text"
		tools:layout_editor_absoluteX="220dp"
		tools:layout_editor_absoluteY="260dp"/>

	<android.support.v7.widget.Toolbar
		android:id="@+id/toolbar"
		android:layout_width="0dp"
		android:layout_height="wrap_content"
		android:background="#C4ADF9"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintRight_toRightOf="parent"
		app:layout_constraintTop_toTopOf="parent"/>
</android.support.constraint.ConstraintLayout>